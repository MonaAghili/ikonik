{
  "name": "ikonik",
  "version": "1.0.0",
  "description": "SVG to React icon generator - tree-shakeable, accessible, customizable",
  "type": "module",
  "author": "Mona Aghili",
  "repository": {
    "type": "git",
    "url": "https://github.com/MonaAghili/ikonik.git"
  },
  "bugs": {
    "url": "https://github.com/MonaAghili/ikonik/issues"
  },
  "homepage": "https://github.com/MonaAghili/ikonik#readme",
  "bin": {
    "icongen": "./dist/cli.cjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/generate.js",
      "require": "./dist/generate.cjs"
    }
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "generator",
    "tree-shaking",
    "typescript",
    "icon-generator",
    "svg-to-react",
    "icon-components"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "change-case": "^5.4.4",
    "commander": "^12.0.0",
    "globby": "^14.0.0",
    "handlebars": "^4.7.8",
    "prettier": "^3.2.0",
    "svgo": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}